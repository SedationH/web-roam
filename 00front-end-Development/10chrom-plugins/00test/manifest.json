{
  "manifest_version": 2,
  "name": "SedationH",
  "description": "SedationH's First Chrome Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "lib/jquery.js",
        "content.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html",
    "default_title": "This is popup Title"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}